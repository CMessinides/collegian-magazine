<li class="search__results-section">
  <h2>Staff <span class="search__results-count">{{=it.staff.length}}</span></h2>
  <ul>
    {{~it.staff :value:index}}
      <li>
        <article class="card">
          {{? value.title }}<h3 class="title">{{=value.title}}</h3>{{?}}
        </article>
      </li>
    {{~}}
  </ul>
</li>
